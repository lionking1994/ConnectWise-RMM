{
  "name": "rmm-integration-backend",
  "version": "1.0.0",
  "description": "ConnectWise and N-able RMM Integration Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "ts-node src/test-api-connection.ts",
    "test:connection": "ts-node src/test-api-connection.ts",
    "test:nsight": "node test-nsight-api.js",
    "test:apis": "node ../test-api-connections.js",
    "test:workflow": "bash ../test-full-workflow.sh",
    "test:integration": "npm run test:apis && npm run test:workflow",
    "lint": "eslint src/**/*.ts",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "psql $DATABASE_URL -f src/database/seed.sql",
    "seed:minimal": "ts-node src/database/seed-minimal.ts",
    "seed:credentials": "ts-node src/database/seed-credentials.ts",
    "seed:dev": "npm run seed:minimal && npm run seed:credentials && npm run seed",
    "setup": "npm run migrate && npm run seed:minimal",
    "verify": "ts-node src/verify-setup.ts",
    "test:setup": "ts-node src/verify-setup.ts"
  },
  "keywords": [
    "connectwise",
    "nable",
    "rmm",
    "integration",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@slack/webhook": "^7.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cron": "^2.0.1",
    "@types/pdfkit": "^0.17.3",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "p-queue": "^7.4.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "retry": "^0.13.1",
    "socket.io": "^4.6.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.20",
    "@types/retry": "^0.12.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
